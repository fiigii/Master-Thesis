\def\beforepreface{
	\ifdblabstract   % the top page is a good place for this
		\pagestyle{empty}
		\abstract% \endabstract
	\fi
	\pagenumbering{roman}
	\pagestyle{plain}
% 1st the manuscript title page
	\manuscriptp
% 2nd the Official Approval page
  %% csun: starting from 2015, this is no longer needed.
	% \officialaprovp
% 3rd the Abstract page
	\abstract \endabstract
% 4th the OPTIONAL copy right page
\addtocounter{pagetemp}{1}
	\ifcopyright\copyrightpage\fi
	}

\def\prefacesection#1{%
	\chapter*{#1}
	\addcontentsline{toc}{chapter}{#1}}

\def\afterpreface{\newpage
	\tableofcontents
	\newpage
	\iffigurespage
		{\addvspace{10pt}
		\let\saveaddvspace=\addvspace
		\def\addvspace##1{}
		\listoffigures
		\let\addvspace=\saveaddvspace}
		\newpage
	\fi
	\iftablespage
		{\addvspace{10pt}
		\let\saveaddvspace=\addvspace
		\def\addvspace##1{}
		\listoftables
		\let\addvspace=\saveaddvspace}
		\newpage
	\fi
        \ifackspage
               {\addvspace{10pt}
                \let\saveaddvspace=\addvspace
		\def\addvspace##1{}
		\textbf{\Large\hfill{ACKNOWLEDGMENTS}\hfill\hbox{}}\par
                \bigskip
                \@AcksText
		\let\addvspace=\saveaddvspace}
		\newpage
	\fi
	\pagenumbering{arabic}
  %% \pagestyle{uwmheadings}
  %% csun: starting from 2015, page number is required to be
  %% at the center bottom of the page.
	\pagestyle{plain}}
